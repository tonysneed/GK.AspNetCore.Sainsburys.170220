@using System.Security.Claims
@model Claim[]

@{
    ViewData["Title"] = "Secure";
}

<div class="container" id="main">
    <h2>User Claims</h2>
    <dl>
        @foreach (var claim in User.Claims)
        {
            <dt>@claim.Type</dt>
            <dd>@claim.Value</dd>
        }
    </dl>
</div>
